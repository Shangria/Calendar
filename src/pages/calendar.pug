.calendar
    .calendar__box
        select.calendar__list-person
            option(value='All members') All members
            option(value='Maria') Maria
            option(value='Bob') Bob
            option(value='Alex') Alex
        .calendar__event-button New event+
    table.calendar
        thead.calendar__head-table
            tr.calendar__row-table
                - allTages = ['Name','Monday','Tuesday','Wednesday','Thursday','Friday'];
                each tag in allTages
                    td.calendar__cell-table=tag
        tbody.calendar__body-table
            - for (let row=0; row<=8;row++)
                tr.calendar__row-table
                    - selectTime=['10:00','11:00','12:00','13:00','14:00','15:00','16:00','17:00','18:00',];
                        - for (let time = 0; time < 6; time++)
                            if time === 0
                                td.calendar__cell-table=selectTime[row]
